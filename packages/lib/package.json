{
  "name": "@penxle/lib",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./environment": {
      "import": "./dist/environment.js",
      "types": "./dist/environment.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "codegen": "svelte-kit sync",
    "dev": "tsup --watch",
    "lint:typecheck": "tsc"
  },
  "dependencies": {
    "@pnpm/find-workspace-dir": "^7.0.0",
    "svelte": "^4.2.14",
    "svgo": "^3.2.0",
    "vite": "^5.2.8"
  },
  "devDependencies": {
    "@penxle/tsconfig": "workspace:^",
    "@sveltejs/kit": "^2.5.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0"
  }
}
