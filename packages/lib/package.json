{
  "name": "@penxle/lib",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./environment": {
      "import": "./dist/environment.js",
      "types": "./dist/environment.d.ts"
    },
    "./unocss": {
      "import": "./dist/unocss.js",
      "types": "./dist/unocss.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "codegen": "svelte-kit sync",
    "dev": "tsup --watch",
    "lint:typecheck": "tsc"
  },
  "dependencies": {
    "@iconify/json": "^2.2.113",
    "@iconify/utils": "^2.1.9",
    "@sindresorhus/string-hash": "^2.0.0",
    "@unocss/config": "^0.55.7",
    "@unocss/core": "^0.55.7",
    "@unocss/preset-icons": "^0.55.7",
    "@unocss/preset-uno": "^0.55.7",
    "browserslist": "^4.21.10",
    "lightningcss": "^1.21.7",
    "magic-string": "^0.30.3",
    "object-traversal": "^1.0.1",
    "svelte": "^4.2.0",
    "svgo": "^3.0.2",
    "vite": "^4.4.9"
  },
  "devDependencies": {
    "@penxle/tsconfig": "workspace:^",
    "@sveltejs/kit": "^1.24.1",
    "rollup": "^3.29.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0"
  }
}
