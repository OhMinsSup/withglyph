{
  "name": "@penxle/lib",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./environment": {
      "import": "./dist/environment.js",
      "types": "./dist/environment.d.ts"
    },
    "./unocss": {
      "import": "./dist/unocss.js",
      "types": "./dist/unocss.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "codegen": "svelte-kit sync",
    "dev": "tsup --watch",
    "lint:typecheck": "tsc"
  },
  "dependencies": {
    "@pnpm/find-workspace-dir": "^6.0.3",
    "@sindresorhus/string-hash": "^2.0.0",
    "@unocss/config": "^0.58.5",
    "@unocss/core": "^0.58.5",
    "@unocss/preset-uno": "^0.58.5",
    "@unocss/vite": "^0.58.5",
    "browserslist": "^4.23.0",
    "css-tree": "^2.3.1",
    "lightningcss": "^1.24.0",
    "magic-string": "^0.30.7",
    "object-traversal": "^1.0.1",
    "svelte": "^4.2.12",
    "svgo": "^3.2.0",
    "vite": "^5.1.4"
  },
  "devDependencies": {
    "@penxle/tsconfig": "workspace:^",
    "@sveltejs/kit": "^2.5.2",
    "@types/css-tree": "^2.3.6",
    "rollup": "^4.12.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0"
  }
}
