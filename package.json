{
  "name": "@penxle/penxle",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "scripts": {
    "bootstrap": "pnpm install && husky install",
    "dev": "doppler run --command 'vite dev'",
    "build": "vite build",
    "sync": "pnpm run db:migrate && pnpm run codegen",
    "codegen": "pnpm run codegen:kit && pnpm run codegen:db && pnpm run codegen:gql",
    "codegen:kit": "doppler run --command 'pnpm exec svelte-kit sync'",
    "codegen:db": "doppler run --command 'pnpm exec kysely-codegen --camel-case --dialect mysql --url $DATABASE_URL\\?ssl={} --out-file \\$kysely/index.d.ts'",
    "codegen:gql": "houdini generate --pull-schema",
    "db:migrate": "doppler run --command 'pnpm dbmate --url $DATABASE_URL\\?tls=true up'",
    "db:new": "dbmate new",
    "lint": "eslint --max-warnings=0 .",
    "format": "eslint --fix .",
    "check": "svelte-check --fail-on-warnings --compiler-warnings 'a11y-aria-attributes:ignore,a11y-incorrect-aria-attribute-type:ignore,a11y-unknown-aria-attribute:ignore,a11y-hidden:ignore,a11y-misplaced-role:ignore,a11y-unknown-role:ignore,a11y-no-abstract-role:ignore,a11y-no-redundant-roles:ignore,a11y-role-has-required-aria-props:ignore,a11y-accesskey:ignore,a11y-autofocus:ignore,a11y-misplaced-scope:ignore,a11y-positive-tabindex:ignore,a11y-invalid-attribute:ignore,a11y-missing-attribute:ignore,a11y-img-redundant-alt:ignore,a11y-label-has-associated-control:ignore,a11y-media-has-caption:ignore,a11y-distracting-elements:ignore,a11y-structure:ignore,a11y-mouse-events-have-key-events:ignore,a11y-missing-content:ignore,a11y-click-events-have-key-events:ignore'",
    "test": "playwright test"
  },
  "devDependencies": {
    "@envelop/core": "^3.0.6",
    "@envelop/types": "^3.0.2",
    "@felte/core": "^1.3.7",
    "@felte/validator-zod": "^1.0.13",
    "@graphql-yoga/plugin-graphql-sse": "^1.5.1",
    "@iconify-json/lucide": "^1.1.90",
    "@iconify-json/ri": "^1.1.8",
    "@iconify/utils": "^2.1.5",
    "@playwright/test": "^1.33.0",
    "@pothos/core": "^3.30.0",
    "@pothos/plugin-dataloader": "^3.17.1",
    "@pothos/plugin-scope-auth": "^3.19.1",
    "@pothos/plugin-simple-objects": "^3.6.8",
    "@pothos/plugin-validation": "^3.9.1",
    "@svelte-kits/store": "^0.1.1",
    "@sveltejs/adapter-vercel": "^2.4.3",
    "@sveltejs/kit": "^1.16.3",
    "@trpc/client": "^10.26.0",
    "@trpc/server": "^10.26.0",
    "@types/node": "^20.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@unocss/eslint-plugin": "^0.51.12",
    "@vercel/analytics": "^1.0.1",
    "@vercel/edge": "^0.3.4",
    "argon2": "^0.30.3",
    "autoprefixer": "^10.4.14",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "dbmate": "^2.3.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-svelte": "^2.28.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "felte": "^1.2.7",
    "gitmoji-cli": "^8.2.2",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.21.3",
    "graphql-sse": "^2.1.2",
    "graphql-yoga": "^3.9.1",
    "houdini": "^1.2.3",
    "houdini-svelte": "^1.2.3",
    "husky": "^8.0.3",
    "jose": "^4.14.4",
    "kysely": "^0.24.2",
    "kysely-codegen": "^0.10.0",
    "kysely-planetscale": "^1.3.0",
    "lint-staged": "^13.2.2",
    "mysql2": "^3.3.1",
    "nanoid": "^4.0.2",
    "network-information-types": "^0.1.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "radash": "^10.8.1",
    "sass": "^1.62.1",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "svelte-preprocess": "^5.0.3",
    "svgo": "^3.0.2",
    "typescript": "^5.0.4",
    "unocss": "^0.51.12",
    "vite": "^4.3.5",
    "web-vitals": "^3.3.1",
    "zod": "^3.21.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "kysely-codegen>mysql2": "^3.0.0"
      }
    }
  }
}
